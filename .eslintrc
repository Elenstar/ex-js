{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "linebreak-style": ["error", "windows"],
    "valid-jsdoc": "off",
    "require-jsdoc": "off",
    "indent": ["error", 4],
    "comma-dangle": "off",
    "no-cond-assign": ["error", "always"],
    "operator-linebreak": "off",
    "no-unused-vars": "off",
    "no-debugger": "off"
  },
  "extends": [
    "eslint:recommended", "google"
  ]
}
